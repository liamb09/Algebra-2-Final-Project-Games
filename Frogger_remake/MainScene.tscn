script/source = "extends Node2D

var enemy = load(\"res://Enemy.tscn\")
var timer = 0
var init_pos_x = 16
var init_pos_y = 100

func _ready():
	while init_pos_x < get_viewport().size.x:
		var enemyinstance = enemy.instance()
		enemyinstance.position.y = init_pos_y
		enemyinstance.position.x = init_pos_x
		add_child(enemyinstance)
		init_pos_x += 200

func _process(delta):
	timer += 1
	if timer == 400:
		var enemy = load(\"res://Enemy.tscn\")
		var enemyinstance = enemy.instance()
		enemyinstance.position.y = init_pos_y
		add_child(enemyinstance)
		timer = 0
"

